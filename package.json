{
  "name": "ReactReduxStarter",
  "version": "0.0.1",
  "description": "Starter template for react redux",
  "main": "''",
  "scripts": {
    "build": "webpack --config webpack.production.config.js --progress --colors",
    "start": "webpack-dev-server --progress --profile --colors --watch --open",
    "lint": "eslint -f compact --ext js --ext jsx src/",
    "lint:fix": "eslint --ext js --ext jsx src --fix",
    "lint:teamcity": "eslint -f ./node_modules/eslint-teamcity/index.js --ext js --ext jsx src",
    "lint:html": "eslint -o reports/eslint-report.html -f node_modules/eslint-html-reporter/reporter-team-city.js --ext js --ext jsx src",
    "plato": "node node_modules/plato/bin/plato -r -e .eslintrc.json -d reports/plato src/",
    "test": "jest --config jest.config.json --coverage --updateSnapshot",
    "test:watch": "jest --config jest.config.json --coverage --updateSnapshot --watch",
    "test:watchAll": "jest --config jest.config.json --coverage --updateSnapshot --watchAll"
  },
  "dependencies": {
    "assignment": "2.2.1",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "aspnet-webpack": "2.0.3",
    "aspnet-webpack-react": "3.0.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1",
    "babel-plugin-syntax-do-expressions": "6.13.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-do-expressions": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "copy-webpack-plugin": "4.3.1",
    "css-loader": "0.28.9",
    "deep-freeze": "0.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.16.0",
    "eslint-html-reporter": "0.5.2",
    "eslint-loader": "1.9.0",
    "eslint-plugin-jest": "21.7.0",
    "eslint-plugin-react": "7.6.1",
    "eslint-teamcity": "2.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.15.0-rc.8",
    "jest": "22.1.1",
    "jest-teamcity-reporter": "0.9.0",
    "lodash.xorby": "4.7.0",
    "node-sass": "4.6.1",
    "plato": "1.7.0",
    "postcss-loader": "2.0.10",
    "raf": "3.4.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.1.1",
    "redux-devtools-extension": "2.13.2",
    "sass-loader": "6.0.6",
    "strip-ansi": "4.0.0",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-dashboard": "1.1.1",
    "webpack-dev-server": "2.11.1",
    "webpack-hot-middleware": "2.22.2"
  },
  "eslint-teamcity": {
    "report-name": "ESLint Violations",
    "error-statistics-name": "ESLint Error Count",
    "warning-statistics-name": "ESLint Warning Count"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test && npm run lint"
    }
  }
}
